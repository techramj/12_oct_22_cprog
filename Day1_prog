program 3:
//wap to check the number is postive or negative
#include<stdio.h>
void main() {
	int num1 = 10;

	if (num1 >= 0) {
		printf("%d is postive", num1);
	}

	if (num1 < 0) {
		printf("%d is negative", num1);
	}
	    
}

#include<stdio.h>
//check the number is postive or negative
void main() {
	int num1 = 10;

	if (num1 >= 0) {
		printf("%d is postive", num1);
	}
	else {
		printf("%d is negative", num1);
	}    
}
