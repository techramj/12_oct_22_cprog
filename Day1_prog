#include<stdio.h>
//Write a program to add two number
void main() {
	int num1, num2, result;
	printf("enter number :");
	scanf_s("%d", &num1);
	printf("enter number :");
	scanf_s("%d", &num2);

	result = num1 + num2;
	printf("result = %d", result);
}

