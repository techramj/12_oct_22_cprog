program 4:
#include<stdio.h>
//print the maximum number among 3
void main() {
	int num1 = 10;
	int num2 = 10;
	int num3 = 10;

	if (num1 > num2 && num1 > num3) {
		printf("%d is greatest", num1);
	}
	else if (num2 > num3) {
		printf("%d is greatest", num2);
	}
	else {
		printf("%d is greatest", num3);
	}
	   
}
---------------------------------------------------
program 5:
wap to print the grade based on mark
mark >=80 and <=100 Grade:A
mark >=60 and <80  Grade:B
mark >=40 and <60 Grade:C
mark >=0 and <40  Grade:Fail

void main() {
	int mark = 10;
	char grade;

	if (mark >= 80 ) {
		grade = 'A';
	}else if (mark >= 60 ) {
		grade = 'B';
	}else if (mark >40 ) {
		grade = 'C';
	}else {
		grade = 'F';
	}

	printf("Grade=%c", grade);

	
}
