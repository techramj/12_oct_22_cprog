program 3:
//wap to check the number is postive or negative using ternary operator

condtion ? exp1 : exp2;
void main() {
	int num = 10;
	int res;
	if (num >= 0) {
		res = 1;
	}
	else {
		res = 0;
	}

	res = num >= 0 ? 1 : 0;
	printf("%d", res);


}



#include<stdio.h>
void main() {
	const int x = 100;
	int month = 110;
	int a = 4;



	switch (month) {
	case 1:printf("Jan"); 
		break;
	case 2:printf("Feb");
		break;
	case 3:printf("Mar");
		break;
	case 4:printf("Apr");
		break;
	case 5:printf("May");
		break;
	case 6:printf("Jun");
		break;
	case 7:printf("Jul");
		break;
	case 8:printf("Aug");
		break;
	case 9:printf("Sep");
		break;
	case 10:printf("oct");
		break;
	case 11:printf("nov");
		break;
	case 12:printf("dec");
		break;
	default: printf("Invalid month");	
	}

}


#include<stdio.h>
void main() {
	 int num = 1;
	 while (num <= 10) {
		 printf("%d ", num);
		 num++;
	 }
	 printf("\n");
	 for (num = 1; num <= 10; num++) {
		 printf("%d ", num);
	 }
	 printf("\n");

	 num = 1;
	 do {
		 printf("%d ", num);
		 num++;
	 } while (num<=10);
}

